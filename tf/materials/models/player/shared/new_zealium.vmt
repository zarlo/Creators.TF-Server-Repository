"VertexlitGeneric"
{
	"$basetexture" "models/player/shared/new_zealium"
	"$bumpmap" "models/weapons/c_items/c_xms_cold_shoulder_normal"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6
	"$yellow" "0"

	"$phong" "1"
	"$phongexponent" "200"
	"$phongboost" "100"
	"$phongfresnelranges"	"[.2  .5 10]"
	"$basemapalphaphongmask" "1"
	//"$phongwarptexture" "models/player/shared/ice_player_warp"
	"$lightwarptexture" "\models\lightwarps\weapon_lightwarp"

               "$envmap" "cubemaps/cubemap_newzealium"
               $envmapfresnel 0.4
	"$envmaptint" "[4 4 4]" 


	// Rim lighting parameters
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "50"									// Exponent for phong component of rim lighting	
	"$rimlightboost" "15"										// Boost for ambient cube component of rim lighting

	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1" "$yellow"
			"resultVar" "$color2"
		}
	}
}