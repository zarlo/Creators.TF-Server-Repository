"VertexlitGeneric"
{
	"$baseTexture" "models\weapons\new_zealium/c_ambassador_opt"
	$bumpmap "models\weapons\c_items/c_ambassador_opt_normal"
	//$halflambert 1

	"$basemapalphaphongmask" 1
	"$phong" "1"
               "$phongexponent" "300"
               "$phongboost" "80"
	"$lightwarptexture" "models\weapons\c_items/c_ambassador_lightwarp"
               "$phongfresnelranges" "[2.2 3.7 4]"
	"$phongwarptexture"  "models\weapons\c_items/c_ambassador_phongwarp"

               "$envmap" "cubemaps/cubemap_newzealium"
               $envmapfresnel 0.2
               "$envmaptint" "[ 10 10 10 ]"

	"$rimlight" "1"
	"$rimlightexponent" "30"	
	"$rimlightboost" "2"

	"360?$color2" "[ 0.9 0.8 0.8 ]"


	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}